{
  "presets": [
    ["@babel/preset-env", {
      "loose": false,
      "modules": false,
      "targets": {
        "browsers": [
          "last 2 versions",
          "> 1%",
          "not dead",
          "not ie 11"
        ],
        "node": "14"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining"
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }]
      ],
      "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }]
      ]
    }
  }
}