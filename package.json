{
  "name": "xajs",
  "version": "1.0.26",
  "description": "ðŸ’— A collection of utility libraries used by @qddegtya",
  "main": "./dist/ajs.cjs.js",
  "module": "./dist/ajs.es.js",
  "browser": "./dist/ajs.umd.js",
  "exports": {
    ".": {
      "browser": "./dist/ajs.umd.js",
      "require": "./dist/ajs.cjs.js",
      "import": "./dist/ajs.es.js",
      "default": "./dist/ajs.cjs.js"
    }
  },
  "type": "module",
  "packageManager": "npm@10.8.2",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qddegtya/ajs.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint **/*.js --fix",
    "build": "rimraf ./dist && ./node_modules/.bin/rollup -c",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "security-check": "npm audit"
  },
  "keywords": [
    "ajs",
    "util",
    "utility",
    "xajs"
  ],
  "author": "xiaoa",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "eslint": "^5.14.1",
    "jest": "^29.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^4.30.1"
  }
}
